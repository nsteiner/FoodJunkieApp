<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DefaultRecipesFragment">

    <TextView
        android:id="@+id/tv_defaultRecipes"
        android:layout_width="270dp"
        android:layout_height="124dp"
        android:text="FOOD JUNKIE RECIPES"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.049" />

    <Button
        android:id="@+id/btn_defaultBreakfast"

        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Breakfast"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_defaultRecipes"
        app:layout_constraintVertical_bias="0.098" />

    <Button
        android:id="@+id/btn_defaultLunch"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Lunch"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_defaultDinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_defaultBreakfast"
        app:layout_constraintVertical_bias="0.55" />

    <Button
        android:id="@+id/btn_defaultDinner"
        android:layout_width="166dp"
        android:layout_height="81dp"

        android:text="Dinner"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_defaultSnacks"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_defaultBreakfast"
        app:layout_constraintVertical_bias="0.872" />

    <Button
        android:id="@+id/btn_defaultSnacks"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Snacks"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_defaultDessert"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_defaultBreakfast"
        app:layout_constraintVertical_bias="0.915" />

    <Button
        android:id="@+id/btn_defaultDessert"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Dessert"

        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_defaultBreakfast"
        app:layout_constraintVertical_bias="0.811" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="285dp"
        android:layout_height="434dp"
        android:layout_marginStart="180dp"
        android:src="@drawable/bowl3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.24" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="277dp"
        android:layout_height="281dp"
        android:layout_marginEnd="168dp"
        android:src="@drawable/bowl0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.961" />

</androidx.constraintlayout.widget.ConstraintLayout>

