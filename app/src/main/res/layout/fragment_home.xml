<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyRecipesFragment">
    <Button
        android:id="@+id/btn_newRecipe"
        android:layout_width="88dp"
        android:layout_height="63dp"
        android:layout_marginStart="16dp"
        android:text="@string/new_recipe"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_myRecipes"
        app:layout_constraintHorizontal_bias="0.0"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082" />
    <TextView
        android:id="@+id/tv_myRecipes"
        android:layout_width="163dp"
        android:layout_height="118dp"
        android:text="My Cookbook"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.049" />
    <Button
        android:id="@+id/btn_myBreakfast"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Breakfast"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_myRecipes"

        app:layout_constraintVertical_bias="0.098" />
    <Button
        android:id="@+id/btn_myLunch"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Lunch"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_myDinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_myBreakfast"
        app:layout_constraintVertical_bias="0.55" />
    <Button
        android:id="@+id/btn_myDinner"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Dinner"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_mySnacks"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_myBreakfast"
        app:layout_constraintVertical_bias="0.872" />
    <Button

        android:id="@+id/btn_mySnacks"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Snacks"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_myDessert"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_myBreakfast"
        app:layout_constraintVertical_bias="0.915" />
    <Button
        android:id="@+id/btn_myDessert"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:text="Dessert"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_myBreakfast"
        app:layout_constraintVertical_bias="0.811" />
    <Button
        android:id="@+id/LogOut"
        android:layout_width="88dp"
        android:layout_height="63dp"

        android:text="SIgn out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082" />
</androidx.constraintlayout.widget.ConstraintLayout>